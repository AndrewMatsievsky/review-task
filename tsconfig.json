{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "declarationDir": "dist/types",
    "types": ["node"]
  },
  "include": [
    "*",
    "components/round/round.injector.ts",
    "components/round/round.processor.ts",
    "components/round/round.state.ts",
    "components/round/round.service.ts"
  ],
  "exclude": ["node_modules"]
}
